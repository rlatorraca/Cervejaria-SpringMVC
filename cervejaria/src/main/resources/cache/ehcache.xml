<?xml version="1.0" encoding="UTF-8"?>
<ehcache:config xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
	xmlns:ehcache='http://www.ehcache.org/v3'
	xsi:schemaLocation="http://www.ehcache.org/v3 http://www.ehcache.org/schema/ehcache-core-3.4.xsd">

	<ehcache:cache alias="cidades">
		<ehcache:expiry>
			<ehcache:tti unit="seconds">90</ehcache:tti>
		</ehcache:expiry>
		<ehcache:resources>
			<ehcache:heap unit="entries">10</ehcache:heap>
		</ehcache:resources>
	</ehcache:cache>
</ehcache:config>